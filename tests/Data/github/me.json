{
    "title": "Github Account",
    "action": "Connect to a Github account",
    "desc": "Collect and sync my data from my Github account.",
    "run": "node ../../Common/node/connector/client_init.js",
    "status": "unstable",
    "handle": "github",
    "provides": [
        "contact/github"
    ],
    "mongoCollections": [
        "repos",
        "followers",
        "following"
    ],
    "processOptions": {
        "oauth2": {
            "provider": "Github",
            "appIDName": "Client ID",
            "appSecretName": "Secret",
            "authEndpoint": "authorize",
            "accessTokenResponse": "json",
            "endPoint": "https://github.com/login/oauth",
            "linkToCreate": "https://github.com/account/applications/new"
        }
    },
    "srcdir": "Connectors/GitHub",
    "is": "connector",
    "installable": true,
    "id": "github",
    "uri": "http://localhost:8043/Me/github/",
    "externalUri": "http://localhost:8043/Me/github/",
    "version": 1
}