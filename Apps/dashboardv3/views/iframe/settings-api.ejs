    <script src="common/js/jquery.jsonp-2.2.0.min.js"></script>
    <script src="js/settings.js"></script>
    <script src="<%= dashboard.customHostedEndpoint %>"></script>
  </head>
  <body>
    <div class="token">
      <input type='text' id="apiToken" name='apiToken' value=''
